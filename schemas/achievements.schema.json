{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["id", "name", "type", "value"],
    "properties": {
      "id": { "type": "string" },
      "name": { "type": "string" },
      "type": { "type": "string" },
      "value": { "type": "number" },
      "flavor": { "type": "string" },
      "secret": { "type": "boolean" }
    }
  }
}
